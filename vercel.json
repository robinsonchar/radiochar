{
  "version": 2,
  "builds": [
    {
      "src": "api/generate-comment.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/generate-comment",
      "dest": "api/generate-comment.js"
    },
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}



